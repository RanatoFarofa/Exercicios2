		/*
		 * int x = 10;
		 * 
		 * while (x < 26) { System.out.println(x); x++;
		 * 
		 * }
		 */

		while (true) {

			int x = 1;
			int y = 2;
			int somar2=0;
			
			if (somar2 < 100) {
				

				int somar = x + y;
				System.out.println(somar);
				somar2 = somar +2; 
				System.out.println(somar2);

			} else {

			}

		}

	}

}
